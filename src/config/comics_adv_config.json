{
  "name": "comics adv config",
  "n_gpu": 1,
  "preprocessing": {
  },
  "generator": {
    "type": "Generator",
    "args": {
      "in_size": 3,
      "n_filter": 64,
      "n_blocks": 9,
      "enc_kernels": [7, 3, 3],
      "enc_stride": 1,
      "enc_padding": [0, 1, 1],
      "dec_kernels": [3, 3],
      "dec_stride": 2,
      "dec_padding": [1, 1]
    }
  },
  "discriminator": {
    "type": "Discriminator",
    "args": {
      "channels": [3, 64, 128, 256, 512],
      "kernel_sz": 4,
      "padding": 1
    }
  },
  "dataset": {
    "img_folder": "/Vol1/dbstore/MDAL/HSE/face2comics_v1.0.0_by_Sxela",
    "parts_train": ["face", "comics"],
    "batch_size_train": 8,
    "batch_size_val": 16
  },
  "optimizer": {
    "type": "Adam",
    "args": {
      "lr": 4e-4,
      "betas": [0.9, 0.999]
    }
  },
  "loss": {
    "type": "CycleGanLoss",
    "args": {
      "types": ["cycle", "id", "adversarial"],
      "discr_loss_coef": 0.5
    },
    "lambda_cyc": 10.0,
    "lambda_id": 5.0
  },
  "trainer": {
    "epochs": 70,
    "save_dir": "saved/",
    "save_period": 10,
    "verbosity": 2,
    "log_step": 10,
    "monitor": "min generator_loss",
    "early_stop": 100,
    "visualize": "tensorboard"
  }
}
