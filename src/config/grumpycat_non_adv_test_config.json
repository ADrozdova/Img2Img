{
  "name": "grumpycat non adv config",
  "n_gpu": 1,
  "preprocessing": {
  },
  "generator": {
    "type": "Generator",
    "args": {
      "in_size": 3,
      "n_filter": 64,
      "n_blocks": 4,
      "enc_kernels": [7, 3, 3],
      "enc_stride": 1,
      "enc_padding": [0, 1, 1],
      "dec_kernels": [3, 3],
      "dec_stride": 2,
      "dec_padding": [1, 1]
    }
  },
  "dataset": {
    "img_folder": "grumpifycat",
    "url": "http://efrosgans.eecs.berkeley.edu/cyclegan/datasets/grumpifycat.zip",
    "parts_train": ["trainA", "trainB"],
    "batch_size_train": 2,
    "batch_size_val": 2,
    "resize": 128
  },
  "loss": {
    "type": "CycleGanLoss",
    "args": {
      "adversarial": "False"
    },
    "lambda_cyc": 10.0,
    "lambda_id": 5.0
  },
  "trainer": {
    "epochs": 100,
    "save_dir": "saved/",
    "save_period": 10,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 100
  },
  "test": {
    "checkpoint_file": "./model_best_cats_non_adv_61_epoch.pth",
    "save_dir_A": "./grumpycat_non_adv_outputs/partA",
    "save_dir_B": "./grumpycat_non_adv_outputs/partB",
    "save_dir_A_true": "./grumpycat_non_adv_true/partA",
    "save_dir_B_true": "./grumpycat_non_adv_true/partB",
    "img_folder_A": "./grumpifycat/trainA",
    "img_folder_B": "./grumpifycat/trainB",
    "resize": 128
  }
}
