{
  "name": "group clipstyler config",
  "data": {
    "input": "test_dataset_resized",
    "image_path": ["12.jpg","46.jpg"],
    "output": "outputs_group_clipstyler/",
    "n_trials": 2,
    "use_masks": false
  },
  "clipstyler": {
    "args": {
      "lambda_tv": 2e-3,
      "lambda_patch": 9000,
      "lambda_dir": 500,
      "content_weight": 150,
      "crop_size": 128,
      "num_crops": 64,
      "max_step": 100,
      "lr": 5e-4,
      "thresh": 0.7,
      "get_content": true,
      "texts": ["person to sketch with black pencil background to watercolour",
        "stick as fire sea as watercolour painting"]
    },
    "gram_args": {
      "patch_size_step": [[64, 64]],
      "lambda_gram": [1e-1],
      "patch_rate": [0.5, 0.7]
    }
  },
  "groupvit": {
    "checkpoint_url": "https://github.com/xvjiarui/GroupViT/releases/download/v1.0.0/group_vit_gcc_yfcc_30e-74d335e6.pth",
    "cfg_path": "src/configs/group_vit_gcc_yfcc_30e.yml",
    "vis_modes": ["input_pred_label", "final_group"],
    "dataset": "context"
  }
}
